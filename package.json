{
  "name": "react-parcel-starter",
  "version": "1.0.0",
  "description": "A setup to bootstrap my react apps fast",
  "author": "Szymon Wiszczuk",
  "homepage": "https://golota60.github.io/currency-converter",
  "license": "MIT",
  "dependencies": {
    "antd": "^4.5.0",
    "moment": "^2.27.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html -d build",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:clear": "jest --clearCache",
    "purge": "rm -rf node_modules && rm -rf .cache && rm -rf dist",
    "deploy": "yarn && parcel build src/index.html -d build --public-url /currency-converter/ --no-minify && gh-pages -d build"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^26.0.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "babel-plugin-import": "^1.13.0",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "react-test-renderer": "^16.13.1",
    "sass": "^1.26.2",
    "ts-jest": "^26.1.2",
    "typescript": "^3.8.3"
  }
}
